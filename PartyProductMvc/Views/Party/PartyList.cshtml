@model IEnumerable<PartyProductMvc.Models.Party>

@{
    ViewBag.Title = "PartyList";
}

<h2>PartyList</h2>

<p>
    @Html.ActionLink("Add New Party", "PartyAddEdit")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PartyId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PartyName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PartyId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PartyName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.PartyId }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.PartyId })
        </td>
    </tr>
}

</table>
